{
  "version": 2,
  "builds": [
    {
      "src": "Backend/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Backend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/build/$1"
    }
  ]
}
