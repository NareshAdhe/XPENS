{
  "version": 2,
  "builds": [
    {
      "src": "Frontend/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "Backend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/$1"
    }
  ]
}
